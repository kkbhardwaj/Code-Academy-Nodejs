extends base.pug
block scripts 
    script(src='/static/index.js')

block style     
    style 
        include ../static/style.css

block content
    section#introsection 
        div Welcome to Code Academy 
        div.small  Eat Sleep Code Repeat
    section#missionSection
            h2 Mission of Communities
            div.card
                h3 Development 
                div.card-box
                    div.card-img
                        img(src="/static/img/log1.png")
                    div.card-content
                        p To create beginner friendly repositories.
            div.card
                h3 Support
                div.card-box 
                    div.card-img
                        img(src="/static/img/log1.png")
                    div.card-content
                        p To provide support for begginers.
            div.card
                h3 Awareness
                div.card-box
                    div.card-img
                        img(src="/static/img/log1.png")
                    div.card-content
                        p To promote latest technologies .
                
        
    section#sponsorsection 
        h3 Our Sponsors
        div#sponsors 
            img(src="/static/img/apple.jpg", alt="Sponsor Image",class="spImage")
            img(src="/static/img/google.jpg", alt="Sponsor Image",class="spImage")
            img(src="/static/img/tr.png", alt="Sponsor Image",class="spImage")        